CREATE DATABASE service_db;
\c service_db
CREATE TABLE service_db (id SERIAL NOT NULL,username VARCHAR NOT NULL,idnumber VARCHAR NOT NULL,lastname VARCHAR NOT NULL,firstname VARCHAR NOT NULL,department VARCHAR NOT NULL,cohort1 VARCHAR NOT NULL,course1 VARCHAR,role1 VARCHAR,course2 VARCHAR,role2 VARCHAR,course3 VARCHAR,role3 VARCHAR,course4 VARCHAR,role4 VARCHAR,course5 VARCHAR,role5 VARCHAR,course6 VARCHAR,role6 VARCHAR,course7 VARCHAR,role7 VARCHAR,course8 VARCHAR,role8 VARCHAR);
